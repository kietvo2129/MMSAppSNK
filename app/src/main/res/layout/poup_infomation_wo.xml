<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="350dp"
    android:layout_height="match_parent"
    android:paddingHorizontal="8dp"
    android:orientation="vertical"
    android:background="#008577"
    android:weightSum="11">
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="end|center"
        android:paddingVertical="10dp"
        android:paddingRight="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/checkstand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:background="@drawable/lab"
            android:paddingHorizontal="15dp"
            android:paddingVertical="5dp"
            android:text="Plan"
            android:textColor="#fff" />

        <TextView
            android:id="@+id/back_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/lab_white"
            android:paddingHorizontal="15dp"
            android:paddingVertical="5dp"
            android:text="Back"
            android:textColor="@color/colorpart" />

    </LinearLayout>
    <LinearLayout

        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:paddingVertical="5sp"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="WO No"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:paddingVertical="5sp"
            android:id="@+id/tvwono"
            android:layout_weight="1"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="1111"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:paddingVertical="5sp"
            android:layout_width="match_parent"
            android:text="PO No"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:background="@drawable/boder_b_t"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:id="@+id/tvpono"
            android:background="@drawable/boder_b_t"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:paddingVertical="5sp"
            android:layout_width="match_parent"
            android:text="Routing No"
            android:background="@drawable/boder_b_t"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvlineno"
            android:paddingVertical="5sp"
            android:background="@drawable/boder_b_t"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="Product Code"
            android:gravity="center|left"
            android:paddingVertical="5sp"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvproductcode"
            android:paddingVertical="5sp"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="Product Name"
            android:gravity="center|left"
            android:paddingVertical="5sp"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:background="@drawable/boder_b_t"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvproductname"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:paddingVertical="5sp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="Target"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:background="@drawable/boder_b_t"
            android:paddingVertical="5sp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvtarget"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:background="@drawable/boder_b_t"
            android:textColor="#fff"
            android:paddingVertical="5sp"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="Schedule"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:background="@drawable/boder_b_t"
            android:textColor="#94F5FA"
            android:paddingVertical="5sp"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvschedule"
            android:background="@drawable/boder_b_t"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:paddingVertical="5sp"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="Factory"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#94F5FA"
            android:background="@drawable/boder_b_t"
            android:paddingVertical="5sp"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/factory"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:background="@drawable/boder_b_t"
            android:paddingVertical="5sp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="Startday"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:paddingVertical="5sp"
            android:background="@drawable/boder_b_t"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="1111"
            android:id="@+id/tvstartday"
            android:gravity="center|left"
            android:paddingVertical="5sp"
            android:paddingLeft="10dp"
            android:background="@drawable/boder_b_t"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="2">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:text="End Date"
            android:background="@drawable/boder_b_t"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:paddingVertical="5sp"
            android:textColor="#94F5FA"
            android:layout_height="match_parent">
        </TextView>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:paddingVertical="5sp"
            android:text="1111"
            android:id="@+id/tvenddate"
            android:background="@drawable/boder_b_t"
            android:gravity="center|left"
            android:paddingLeft="10dp"
            android:textColor="#fff"
            android:layout_height="match_parent">
        </TextView>
    </LinearLayout>

</LinearLayout>